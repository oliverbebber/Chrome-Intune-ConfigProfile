  { &quot;file_extension&quot;: &quot;exe&quot;, &quot;domains&quot;: [&quot;example.com&quot;] },
  { &quot;file_extension&quot;: &quot;swf&quot;, &quot;domains&quot;: [&quot;*&quot;] }
]

