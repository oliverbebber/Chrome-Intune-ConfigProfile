      &quot;daily_limit_mins&quot;: 30,
      &quot;last_updated_millis&quot;: &quot;1570223060437&quot;,
      &quot;restriction&quot;: &quot;TIME_LIMIT&quot;
    },
    {
      &quot;app_info&quot;: {
        &quot;app_id&quot;: &quot;pjkljhegncpnkpknbcohdijeoejaedia&quot;,
        &quot;app_type&quot;: &quot;EXTENSION&quot;
      },
      &quot;daily_limit_mins&quot;: 10,
      &quot;last_updated_millis&quot;: &quot;1570223000000&quot;,
      &quot;restriction&quot;: &quot;TIME_LIMIT&quot;
    },
    {
      &quot;app_info&quot;: {
        &quot;app_id&quot;: &quot;iniodglblcgmngkgdipeiclkdjjpnlbn&quot;,
