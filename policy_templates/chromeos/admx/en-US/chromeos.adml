      },
      {
        &quot;class&quot;: &quot;PRIVACY_SCREEN&quot;,
        &quot;level&quot;: &quot;BLOCK&quot;
      },
      {
        &quot;class&quot;: &quot;SCREEN_SHARE&quot;,
        &quot;level&quot;: &quot;REPORT&quot;
      }
    ],
    &quot;sources&quot;: {
      &quot;urls&quot;: [
        &quot;salesforce.com&quot;,
